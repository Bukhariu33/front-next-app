{
  "name": "fund-distribution",
  "version": "3.18.0",
  "description": "Fund Distribution",
  "author": "Owais Development Team",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "build-stats": "cross-env ANALYZE=true pnpm run build",
    "clean": "rimraf .next .swc out coverage",
    "lint": "next lint",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .prettierignore",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "cypress": "node ./scripts/setupTest && cypress open",
    "cypress:install": "cypress install",
    "cypress:headless": "node ./scripts/setupTest && cypress run",
    "e2e": "start-server-and-test dev http://localhost:4000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:4000 cypress:headless",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://localhost:6006 test-storybook",
    "prepare": "husky install",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@mantine/carousel": "^7.1.7",
    "@mantine/core": "^7.1.7",
    "@mantine/dates": "^7.1.7",
    "@mantine/dropzone": "^7.1.7",
    "@mantine/hooks": "^7.1.7",
    "@mantine/modals": "^7.1.7",
    "@mantine/next": "^6.0.21",
    "@mantine/notifications": "^7.1.7",
    "@mantine/nprogress": "^7.1.7",
    "@react-google-maps/api": "^2.19.2",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@testing-library/user-event": "^14.5.1",
    "@tirke/node-cache-manager-ioredis": "^3.4.0",
    "@types/lodash": "^4.14.200",
    "@types/ua-parser-js": "^0.7.38",
    "axios": "^1.6.0",
    "cache-manager": "^5.2.4",
    "clsx": "^2.0.0",
    "cookies-next": "^4.0.0",
    "d3": "^7.8.5",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^7.1.0",
    "formik": "^2.4.5",
    "i18next": "^23.7.19",
    "i18next-chained-backend": "^4.5.0",
    "i18next-http-backend": "^2.3.1",
    "i18next-localstorage-backend": "^4.2.0",
    "jscpd": "^3.5.10",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-auth": "^4.24.4",
    "next-i18next": "^14.0.3",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-i18next": "^13.5.0",
    "react-select": "^5.8.0",
    "recoil": "^0.7.7",
    "sharp": "^0.32.6",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwind-merge": "^1.14.0",
    "ua-parser-js": "^1.0.37",
    "yup": "^1.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@commitlint/cz-commitlint": "^17.8.1",
    "@next/bundle-analyzer": "^13.5.6",
    "@next/eslint-plugin-next": "^13.5.6",
    "@percy/cli": "^1.27.3",
    "@percy/cypress": "^3.1.2",
    "@semantic-release/changelog": "^6.0.3",
